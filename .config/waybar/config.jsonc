{
  "layer": "top",
  "position": "top",
  "spacing": 0,
  "height": 0,
  "margin-top": 0,
  "margin-bottom": 0,
  "margin-left": 0,
  "margin-right": 0,
  "modules-left": [
    "sway/workspaces",
    "tray",
    "sway/mode"
  ],
  "modules-center": [
    "clock"
  ],
  "modules-right": [
    "idle_inhibitor",
    "power-profiles-daemon",
    "cpu",
    "temperature",
    "memory",
    "network",
    "backlight",
    "wireplumber#sink",
    "wireplumber#source",
    "battery",
    "custom/power"
  ],
  "sway/workspaces": {
    "all-outputs": true,
    "disable-scroll": true,
    "persistent-workspaces": {
      "1": [
        "eDP-1"
      ]
    }
  },
  "idle_inhibitor": {
    "format": "idle inhib {icon}",
    "format-icons": {
      "activated": "on",
      "deactivated": "off"
    }
  },
  "power-profiles-daemon": {
    "format": "{icon}",
    "tooltip-format": "Power profile: {profile}\nDriver: {driver}",
    "tooltip": true,
    "format-icons": {
      "default": "",
      "performance": "",
      "balanced": "",
      "power-saver": ""
    }
  },
  "clock": {
    "format": "{:%a %d/%m/%Y %H:%M}",
    "tooltip-format": "<tt><small>{calendar}</small></tt>"
  },
  "tray": {
    "tooltip": false,
    "spacing": 8
  },
  "cpu": {
    "format": "cpu {usage}% ",
    "interval": 2,
    "states": {
      "critical": 90
    }
  },
  "temperature": {
    "interval": 2,
    "critical-threshold": 80,
    "tooltip": false,
    "format": "{temperatureC}°C {icon}",
    "format-icons": [
      "",
      "",
      ""
    ]
  },
  "memory": {
    "format": "mem {percentage}% ",
    "interval": 2,
    "states": {
      "critical": 80
    }
  },
  "battery": {
    "interval": 5,
    "states": {
      "good": 95,
      "warning": 20,
      "critical": 10
    },
    "tooltip": false,
    "format": "{capacity}% {icon}",
    "format-full": "{capacity}% {icon}",
    "format-charging": "{capacity}% ",
    "format-plugged": "{capacity}% ",
    "format-alt": "{time} {icon}",
    "format-icons": [
      "",
      "",
      "",
      "",
      ""
    ]
  },
  "network": {
    "format-wifi": "up  ",
    "format-ethernet": "up  ",
    "format-disconnected": "no internet",
    "tooltip": false
  },
  "backlight": {
    "format": "backlight {icon}",
    "format-icons": [
      "",
      "",
      "",
      "",
      "",
      "",
      "",
      "",
      ""
    ],
    "tooltip": false
  },
  "wireplumber#sink": {
    "format": "{icon} {volume}%",
    "format-muted": "vol muted",
    "scroll-step": 5,
    "tooltip": false,
    "format-icons": [
      "",
      "",
      ""
    ],
    "max-volume": 150
  },
  "wireplumber#source": {
    "node-type": "Audio/Source",
    "format": " {volume}%",
    "format-muted": "",
    "scroll-step": 1,
    "tooltip": false
  },
  "custom/power": {
    "format": "⏻ ",
    "tooltip": false,
    "menu": "on-click",
    "menu-file": "$HOME/.config/waybar/power_menu.xml",
    "menu-actions": {
      "shutdown": "shutdown && notify-send -u critical 'System shutting down in 60 seconds...'",
      "reboot": "reboot",
      "suspend": "systemctl suspend",
      "hibernate": "systemctl hibernate"
    }
  }
}
